    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
  	<% if (includeBootstrap) { %> "bootstrap": "3.3.4", <% } %>
  	<% if (includeFontAwesome) { %> "font-awesome": "4.3.0", <% } %>
